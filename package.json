{
  "name": "minimal-repo-voice",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android:clean": "cd android && ./gradlew clean",
    "android:clean:cache": "cd android && rm -rf .gradle && rm -rf build && rm -rf app/build",
    "android:clean:all": "yarn android:clean && yarn android:clean:cache",
    "android:assemble:debug": "cd android && ./gradlew assembleDebug --stacktrace",
    "prebuild": "expo prebuild --clean --skip-dependency-update react-native"
  },
  "dependencies": {
    "@react-native-voice/voice": "^3.2.4",
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "polished": "^4.3.1",
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "private": true
}
